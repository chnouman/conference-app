<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="24dp"
                android:paddingRight="24dp"
                android:paddingTop="8dp">

    <TextView
        android:id="@+id/text_slot_conflict_message1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/dialog_slot_conflict_message_part1"
        android:textAppearance="@style/TextAppearance.AppCompat.Subhead"/>

    <TextView
        android:id="@+id/text_slot_conflict_old_talk_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/text_slot_conflict_message1"
        android:text="Bla bal kdajklfjdeij fa fkldas"
        android:textAppearance="@style/TextAppearance.AppCompat.Subhead"
        android:textColor="@color/primary"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/text_slot_conflict_message2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/text_slot_conflict_old_talk_title"
        android:layout_marginTop="8dp"
        android:text="@string/dialog_slot_conflict_message_part2"
        android:textAppearance="@style/TextAppearance.AppCompat.Subhead"/>

</RelativeLayout>